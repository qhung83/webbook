{{#with Chitietsanpham}}
<form action="" method="POST" role="form" style="color: gray;">
		<div class="form-group">
		<label for="id">ID</label>
		<input type="text" class="form-control" id="id" name="DetailID" value="{{DetailID}}" readonly="true">
	</div>
	<div class="form-group">
		<label >Mã sản phẩm</label>
		<input type="text" class="form-control" id="" name="ProID" value="{{ProID}}">
	</div>
	<div class="form-group">
		<label for="">Đường kính mặt</label>
		<input type="text" class="form-control" id="Duongkinhmat" name="Duongkinhmat" value="{{Duongkinhmat}}">
	</div>
	<div class="form-group">
		<label for="">Chống nước</label>
		<input type="text" class="form-control" id="Chongnuoc" name="Chongnuoc" value="{{Chongnuoc}}">
	</div>
	<div class="form-group">
		<label for="">Chất liệu mặt</label>
		<input type="text" class="form-control" id="Chatlieumat" name="Chatlieumat" value="{{Chatlieumat}}">
	</div>
	<div class="form-group">
		<label for="">Năng lượng sử dụng</label>
		<input type="text" class="form-control" id="Nangluongsudung" name="Nangluongsudung" value="{{Nangluongsudung}}">
	</div>
	<div class="form-group">
		<label for="">Size dây</label>
		<input type="text" class="form-control" id="Sizeday" name="Sizeday" value="{{Sizeday}}">
	</div>
	<div class="form-group">
		<label for="">Chất liệu dây</label>
		<input type="text" class="form-control" id="Chatlieuday" name="Chatlieuday" value="{{Chatlieuday}}">
	</div>
	<div class="form-group">
		<label for="">Chất liệu vỏ</label>
		<input type="text" class="form-control" id="Chatlieuvo" name="Chatlieuvo" value="{{Chatlieuvo}}">
	</div>
	<div class="form-group">
		<label for="">Kiểu dáng</label>
		<input type="text" class="form-control" id="Kieudang" name="Kieudang" value="{{Kieudang}}">
	</div>
	<div class="form-group">
		<label for="">Xuất xứ</label>
		<input type="text" class="form-control" id="Xuatxu" name="Xuatxu" value="{{Xuatxu}}">
	</div>
	<div class="form-group">
		<label for="">Chế độ bảo hành</label>
		<input type="text" class="form-control" id="Chedobaohanh" name="Chedobaohanh" value="{{Chedobaohanh}}">
	</div>
	<a class="btn btn-outline-secondary" href="/admin/chitietsanpham" role="button">
		<i class="fa fa-arrow-left" aria-hidden="true"></i>
		Danh sách chi tiết sản phẩm
	</a>
	<button type="submit" class="btn btn-outline-success">
		<i class="fa fa-check" aria-hidden="true"></i>
		Sửa
	</button>
</form>
{{/with}}